    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch",
            "type": "lldb",
            "request": "launch",
            "targetArchitecture": "x64",
            "program": "${workspaceRoot}/out/build/${command:cmake.buildType}/${env:OS}/NeuralNetworkd",
